<h1>Events</h1>
<%= link_to "Create Event", new_event_path %>

<table>
  <thead>
    <tr>
      <th>Location</th>
      <th>Date/Time</th>
    </tr>
  </thead>
  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= link_to event.location %></td>
        <td><%= event.occurs_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
